{
    "files.exclude": {
        "lib/*": true,
        "**/.git": true,
        "**/.vscode": true,
        "**/*.log": true,
        "Resources/*": true
    },
    "search.workspaceSymbolKinds": [
        "Function",
        "Method"
    ],
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "editor.lineNumbers": "relative",
    "editor.cursorSurroundingLines": 100,
    "editor.fontSize": 15,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.foldfix": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "h",
                "t"
            ],
            "commands": [
                "workbench.action.gotoSymbol"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.referenceSearch.trigger"
            ],
            "args": []
        },
        {
            "before": [
                "m",
                "n"
            ],
            "commands": [
                "bookmarks.jumpToNext"
            ]
        },
        {
            "before": [
                "m",
                "i"
            ],
            "commands": [
                "bookmarks.toggleLabeled"
            ]
        },
        {
            "before": [
                "m",
                "m"
            ],
            "commands": [
                "bookmarks.toggle"
            ]
        },
        {
            "before": [
                "m",
                "A"
            ],
            "commands": [
                "bookmarks.listFromAllFiles"
            ]
        },
        {
            "before": [
                "m",
                "a"
            ],
            "commands": [
                "bookmarks.list"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "peekTypeDefinition"
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "p"
            ],
            "after": [
                "p",
                "g",
                "v",
                "y"
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "outline.showFunctions": true,
    "outline.showInterfaces": true,
    "outline.showMethods": true,
    "outline.showVariables": false,
    "breadcrumbs.showVariables": false,
    "outline.showArrays": false,
    "outline.showBooleans": false,
    "outline.showClasses": false,
    "outline.showConstants": false,
    "outline.showEnumMembers": false,
    "outline.showNull": false,
    "outline.showNumbers": false,
    "outline.showStrings": false,
    "breadcrumbs.showBooleans": false,
    "breadcrumbs.showArrays": false,
    "breadcrumbs.showConstants": false,
    "breadcrumbs.showEnumMembers": false,
    "breadcrumbs.showNumbers": false,
    "breadcrumbs.showObjects": false,
    "breadcrumbs.showOperators": false,
    "breadcrumbs.showStrings": false,
    "breadcrumbs.showTypeParameters": false,
    "breadcrumbs.symbolSortOrder": "type",
    "redhat.telemetry.enabled": false,
    "[xml]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml"
    },
    "editor.wordWrap": "on",
    "workbench.editor.empty.hint": "hidden",
    "editor.minimap.enabled": false,
    "hediet.vscode-drawio.resizeImages": null,
    "gitlens.codeLens.enabled": false,
    "git.openRepositoryInParentFolders": "never",
    "cmake.showOptionsMovedNotification": false,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": false,
    "window.customTitleBarVisibility": "never",
    "remote.SSH.remotePlatform": {
        "wmss": "linux",
        "aibox": "linux"
    }
}
